@model dynamic

<h5>@T["Custom File"]</h5>

<fieldset class="form-group">
    <div class="row">
        <div class="col-sm">
            <span class="hint">@T["The custom file to add as part of the plan."]</span>
        </div>
    </div>
</fieldset>

<div class="form-group">
    <label id="filename">@T["File Name"]</label>
    <input name="FileName" class="form-control" value="@Model.FileName"/>
</div>

<div class="form-group">
    <label id="filecontent">@T["File Content"]</label>
    <code><textarea name="FileContent" class="form-control">@Model.FileContent</textarea></code>
</div>
